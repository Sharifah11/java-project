node('linux') {
    stage('Unit Tests') {
        git 'https://github.com/CHRI4177/java-project.git'
        sh 'ant -f test.xml -v'
        junit 'reports/result.xml'       
    }
    stage('Build') {
        sh 'ant -f build.xml -v'
    }
    stage('Deploy') {
        sh "aws s3 cp /workspace/java-pipeline/dist/rectangle-${env.BUILD_NUMBER}.jar s3://sharifah11-assignment-4"
    }
    stage('Report'){
        withCredentials([[$class: 'AmazonWebServicesCredentialsBinding', accessKeyVariable: 'AWS_ACCESS_KEY_ID', credentialsId: '', secretKeyVariable: 'AWS_SECRET_ACCESS_KEY']]) {
    // some block
      }
    }
}
